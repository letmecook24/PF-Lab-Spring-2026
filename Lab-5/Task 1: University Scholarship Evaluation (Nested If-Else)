#include <stdio.h>
int main() {
    float CGPA;
    int income;
    printf("Enter your CGPA: ");
    scanf("%f",&CGPA);
     printf("Enter your family income0: ");
    scanf("%d",&income);
    if(CGPA>=3.5)
   {	 if(income<50000)
       {	printf("100%% Scholarship");	}
        else
        {	printf("50%% Scholarship");	}	}
    else
    {	if(CGPA>=3.0 && income<40000)
        {	printf("25%% Scholarship");	 }
        else
        {	printf("No Scholarship");		}	}
    return 0;
}
