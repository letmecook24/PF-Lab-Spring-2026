#include<stdio.h>
int main()
{
    float s1,s2,s3;
    printf("Enter sides of triangle: ");
    scanf("%f%f%f",&s1,&s2,&s3);
    
    if(s1+s2<=s3 || s1+s3<=s2 || s2+s3<=s1)
    {
        printf("Not a valid triangle");
    }
    else
    {	if(s1==s2 && s1==s3)
        	{    printf("Equilateral")	}
        	else
        	{   if(s1==s2 || s1==s3 || s2==s3)
            		{	printf("Isosceles");	}
            	     else
            	{	printf("Scalene");		}				}			}
    return 0;
}
